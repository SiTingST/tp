@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlyAddressBook <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class AddressBook
Class ReadOnlyAddressBook
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Student {
Class Student
Class MatriculationNumber
Class Faculty
Class Address
Class Email
Class Name
Class Phone
Class UniqueStudentList
Class VaccinationStatus
Class MedicalDetails
}

Package SchoolResidence {
Class SchoolResidence
}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" AddressBook
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

AddressBook *--> "1" UniqueStudentList
UniqueStudentList o--> "*" Student
Student *--> Name
Student *--> MatriculationNumber
Student *--> Faculty
Student *--> Phone
Student *--> Email
Student *--> Address
Student *--> VaccinationStatus
Student *--> MedicalDetails
Student *--> "*" SchoolResidence

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager ----->" ~* filtered list" Student
@enduml
