@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title Parsing the Add command


class Logic as "__logic:LogicManager__"

class Command as "__command:Command__"

class AddressBookParser as "__addressBookParser:AddressBookParser__"

class AddCommandParser as "__:AddCommandParser__"

class AddCommand as "__:AddCommand__"

class Person as "__person:Person__"

Logic -down- Command
Command -down- AddressBookParser
AddressBookParser -right- AddCommandParser
AddCommandParser -up- AddCommand
AddCommand -up- Person
@end
